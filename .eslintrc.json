{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "html"],
  "rules": {
    "react/jsx-no-target-blank": "off",
    "react/prop-types": "off",
    "indent": "off",
    "linebreak-style": 0,
    "quotes": ["error", "double"],
    "semi": 0
  }
}
